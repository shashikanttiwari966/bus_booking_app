<div class="nested-fields">
  <div class="row">
    <div class="form-group mb-3 col-sm-6">
      <%= f.label :first_name, class: "mt-3" %>
      <%= f.text_field :first_name, class: 'mb-3 form-control'%>
    </div>
    <div class="form-group mb-3 col-sm-6">
      <%= f.label :last_name, class: "mt-3" %>
      <%= f.text_field :last_name, class: 'mb-3 form-control'%>
    </div>
  </div>

  <div class="row">
    <div class="form-group mb-3 col-sm-6">
      <%= f.label :age, class: "mt-3" %>
      <%= f.text_field :age, class: 'mb-3 form-control'%>
    </div>
    
    <div class="form-group mb-3 col-sm-6">
      <%= f.label :contact_no, class: "mt-3" %>
      <%= f.number_field :contact_no, class: 'mb-3 form-control'%>
    </div>
  </div>

  <div class="form-group mb-3">
    <%= f.label :address, class: "mt-3" %>
    <%= f.text_area :address, class: 'mb-3 form-control'%>
  </div>



  <%= link_to_remove_association "Delete", f %>
</div>